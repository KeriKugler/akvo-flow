<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		/* Add CSS here. See the GWT docs on UI Binder for more details */
		.body{
			background-color:white;
		}
		.important {
			font-weight: bold;
		}
	</ui:style>
	<g:VerticalPanel>
		<g:Label styleName="{style.important}" ui:field="labelFormDesc" ui:text="Standard Scoring Item Detail Form:"/>
		<g:Label styleName="{style.important}" ui:field="labelID" ui:text="ID"/>
		<g:TextBox styleName="{style.important}" ui:field="textID" ui:enabled="false"/>
		<g:Label styleName="{style.important}" ui:field="labelGlobal" ui:text="Global Standard"/>
		<g:ListBox styleName="{style.important}" ui:field="lbGlobal" />
		<g:Label styleName="{style.important}" ui:field="labelCountry" ui:text="Country" ui:visible="false"/>
		<g:ListBox styleName="{style.important}" ui:field="lbCountry" ui:visible="false"/>
		<g:Label styleName="{style.important}" ui:field="labelSubValue" ui:text="Sub-Value" ui:visible="false"/>
		<g:ListBox styleName="{style.important}" ui:field="subValue" ui:visible="false"/>
		<g:Label styleName="{style.important}" ui:field="labelPointType" ui:text="Point Type" />
		<g:ListBox ui:field="pointType" />
		<g:Label styleName="{style.important}" ui:field="labelDisplayName" ui:text="Display Name" />
		<g:TextBox ui:field="displayName" />
		<g:Label styleName="{style.important}" ui:field="labelEvaluateField" ui:text="Evaluate Field" />
		<g:ListBox ui:field="evaluateField" />
		<g:Label styleName="{style.important}" ui:field="labelCriteriaType" ui:text="Criteria Type" />
		<g:ListBox ui:field="criteriaType" />
		<g:Label styleName="{style.important}" ui:field="labelPositiveCriteria" ui:text="Positive Criteria" />
		<g:TextBox ui:field="positiveCriteria" />
		<g:Label styleName="{style.important}" ui:field="labelPositiveCriteriaOperator" ui:text="Positive Criteria Operator" />
		<g:ListBox ui:field="positiveCriteriaOperator" />
		<g:Label styleName="{style.important}" ui:field="labelPositiveScore" ui:text="Positive Score" />
		<g:TextBox ui:field="positiveScore" />
		<g:Label styleName="{style.important}" ui:field="labelPositiveMessage" ui:text="Positive Message" />
		<g:TextBox ui:field="positiveMessage" />
		<g:Label styleName="{style.important}" ui:field="labelOverrideFlag" ui:text="Override Flag" />
		<g:CheckBox ui:field="overrideFlag" />
		<g:HorizontalPanel>		
			<g:Button styleName="{style.important}" ui:field="saveButton" ui:text="Save" ui:enabled="false"/>
			<g:Button styleName="{style.important}" ui:field="cancelButton" ui:text="Cancel" ui:enabled="true"/>
		</g:HorizontalPanel>
	</g:VerticalPanel>
</ui:UiBinder> 